{
  "name": "vscode-web-app",
  "displayName": "vscode-web-app",
  "description": "",
  "publisher": "moritanian",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-web-app.openWebApp",
    "onView:targetTree2"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "vscode-web-app": [
        {
          "id": "targetTree2",
          "name": "Targets"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscode-web-app",
          "title": "vc-web-app",
          "icon": "resources/icon.png"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Cloudlatex",
      "properties": {
        "cloudlatex.outDir": {
          "type": "string",
          "default": ".workspace",
          "description": "directory to output compile result"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/pako": "^1.0.1",
    "@types/puppeteer-core": "^2.0.0",
    "chokidar": "^3.3.1",
    "eventemitter3": "^4.0.0",
    "form-data": "^3.0.0",
    "inline-source": "^7.2.0",
    "karma-chrome-launcher": "^2.2.0",
    "node-fetch": "^2.6.0",
    "pako": "^1.0.11",
    "puppeteer-core": "^2.1.1",
    "which-chrome": "^0.1.1"
  }
}
